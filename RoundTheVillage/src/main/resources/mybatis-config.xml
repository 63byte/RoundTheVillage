<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
  	<setting name="jdbcTypeForNull" value="NULL"/>
  	<!-- DB로 전달되는 값 중 null이 있으면 에러가 아닌 NULL 자체로 인식 -->
  </settings>


	<typeAliases>
		<typeAlias type="com.kh.RoundTheVillage.lesson.model.vo.Lesson" alias="Lesson"/>
		<typeAlias type="com.kh.RoundTheVillage.lesson.model.vo.LessonDetail" alias="LessonDetail"/>
		<typeAlias type="com.kh.RoundTheVillage.lesson.model.vo.LessonFile" alias="LessonFile"/>
		<typeAlias type="com.kh.RoundTheVillage.lesson.model.vo.LessonReview" alias="LessonReview"/>
		<typeAlias type="com.kh.RoundTheVillage.lesson.model.vo.LessonQuestion" alias="LessonQuestion"/>
		
		<typeAlias type="com.kh.RoundTheVillage.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.RoundTheVillage.board.model.vo.Attachment" alias="Attachment"/>
		
		<typeAlias type="com.kh.RoundTheVillage.pay.model.vo.Pay" alias="Pay"/>
		<typeAlias type="com.kh.RoundTheVillage.pay.model.vo.PayLes" alias="PayLes"/>
		<typeAlias type="com.kh.RoundTheVillage.pay.model.vo.Coupon" alias="Coupon"/>
		<typeAlias type="com.kh.RoundTheVillage.banner.model.vo.Banner" alias="Banner"/>
		
		<typeAlias type="com.kh.RoundTheVillage.CScenter.model.vo.Notice" alias="Notice"/>
		<typeAlias type="com.kh.RoundTheVillage.CScenter.model.vo.NoticeAttachment" alias="NoticeAttachment"/>
		
		<typeAlias type="com.kh.RoundTheVillage.shop.model.vo.Shop" alias="Shop"/>
		<typeAlias type="com.kh.RoundTheVillage.shop.model.vo.ShopAttachment" alias="ShopAttachment"/>
		
		<typeAlias type="com.kh.RoundTheVillage.member.model.vo.Member" alias="Member"/>
		
		<typeAlias type="com.kh.RoundTheVillage.manager.model.vo.ManagerReport" alias="ManagerReport"/>
		
	</typeAliases>  

  <mappers>
	  <mapper resource="/mappers/lesson-mapper.xml"/>
	  <mapper resource="/mappers/lesson-mapper2.xml"/>
	  <mapper resource="/mappers/board-mapper.xml"/>
	  <mapper resource="/mappers/pay-mapper.xml"/>
	  <mapper resource="/mappers/banner-mapper.xml"/>
	  <mapper resource="/mappers/notice-mapper.xml"/>
	  <mapper resource="/mappers/shop-mapper.xml"/>
	  <mapper resource="/mappers/member-mapper.xml"/>
	  <mapper resource="/mappers/managerReport-mapper.xml"/>
	  <mapper resource="/mappers/inquiry-mapper.xml"/>
  </mappers>


</configuration>